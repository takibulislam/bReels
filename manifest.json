{
  "manifest_version": 3,
  "name": "BReels",
  "version": "1.1",
  "description": "Blocks Facebook Reels from appearing on the feed.",
  "permissions": ["scripting"],
  "host_permissions": ["https://www.facebook.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["index.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ]
}
